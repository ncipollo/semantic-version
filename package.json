{
  "name": "semantic-version-diff",
  "version": "0.0.1",
  "private": true,
  "description": "An action which manages a github release",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib/*",
    "debug": "yarn clean && yarn install && yarn build",
    "release": "yarn clean && yarn install --production && yarn build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncipollo/release-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Nick Cipollo",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.0",
    "@actions/github": "^1.0.0",
    "@types/node": "^12.0.4",
    "glob": "^7.1.4"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "jest": "^25.1.0",
    "jest-circus": "^25.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.3"
  }
}
