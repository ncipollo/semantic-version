{
  "name": "semantic-version-diff",
  "version": "0.0.1",
  "private": true,
  "description": "An action which manages a github release",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib/*",
    "debug": "yarn clean && yarn install && yarn build",
    "release": "yarn clean && yarn install --production && yarn build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncipollo/release-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Nick Cipollo",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.0.0",
    "@actions/github": "^1.0.0",
    "@types/node": "^12.0.4",
    "glob": "^7.1.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.2.4",
    "jest-circus": "^27.2.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
